{
  "name": "up",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "prestart": "yarn update-stories",
    "postinstall": "patch-package",
    "start": "react-native start",
    "start:native": "yarn start",
    "start:web": "webpack serve --mode=development --config react-native-web.config.js",
    "start:storybook:native": "STORYBOOK=1 yarn start",
    "start:storybook:web": "start-storybook --port 6006",
    "build:android": "react-native run-android",
    "build:ios": "react-native run-ios",
    "build:web": "webpack --config react-native-web.config.js",
    "build:storybook:web": "build-storybook --output-dir public",
    "pod:install": "cd ios && USE_FABRIC=1 RCT_NEW_ARCH_ENABLED=1 pod install",
    "gradle:clean": "cd android && ./gradlew clean",
    "test": "jest",
    "lint": "eslint .",
    "update-stories": "sb-rn-get-stories --config-path .ondevice",
    "storybook-watcher": "sb-rn-watcher --config-path .ondevice"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-community/datetimepicker": "^6.7.0",
    "@react-native-community/slider": "^4.3.3",
    "@react-navigation/native": "^6.0.13",
    "@react-navigation/stack": "^6.3.4",
    "dripsy": "^3.8.1",
    "react": "18.1.0",
    "react-dom": "^18.2.0",
    "react-native": "0.70.5",
    "react-native-gesture-handler": "^2.8.0",
    "react-native-reanimated": "^3.0.0-rc.6",
    "react-native-safe-area-context": "^4.4.1",
    "react-native-screens": "^3.18.2",
    "react-native-web": "^0.18.10"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-export-namespace-from": "^7.18.9",
    "@babel/runtime": "^7.12.5",
    "@faker-js/faker": "^7.6.0",
    "@storybook/addon-actions": "^6.5.13",
    "@storybook/addon-controls": "^6.5.13",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-links": "^6.5.13",
    "@storybook/addon-ondevice-actions": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-backgrounds": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-controls": "^6.0.1-beta.9",
    "@storybook/addon-ondevice-notes": "^6.0.1-beta.9",
    "@storybook/addon-react-native-web": "^0.0.19",
    "@storybook/react": "^6.5.13",
    "@storybook/react-native": "^6.0.1-beta.9",
    "@testing-library/jest-native": "^5.3.0",
    "@testing-library/react-native": "^11.5.0",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@vercel/style-guide": "^4.0.2",
    "babel-jest": "^26.6.3",
    "babel-loader": "8.3.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-react-native-web": "^0.18.10",
    "eslint": "^8.26.0",
    "eslint-plugin-flowtype": "^8.0.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-native-a11y": "^3.3.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.2",
    "husky": "^8.0.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "0.72.3",
    "node-self": "^1.0.1",
    "patch-package": "^6.5.0",
    "react-test-renderer": "^18.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  },
  "prettier": "@vercel/style-guide/prettier"
}
